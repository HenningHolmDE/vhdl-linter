{
  "name": "vhdl-linter",
  "main": "./dist/lib/index.js",
  "version": "0.0.0",
  "description": "A typescript based linter for vhdl",
  "keywords": [],
  "repository": "https://github.com/atom/vhdl-linter",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "build": "tsc --project .",
    "build-watch": "tsc -w --jsx react --project  .",
    "lint": "tslint --project .",
    "prepublishOnly": "npm run build",
    "test": "node dist/test/test.js",
    "watch": "./node_modules/nodemon/bin/nodemon.js -e ts -w ./ -x npm run watch:serve",
    "watch:serve": "./node_modules/ts-node/dist/bin.js test/test.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "post-merge": "npm ci && npm run build && echo '#####################\nRestart Atom\n#####################'"
    }
  },
  "dependencies": {
    "@types/atom": "^1.31.0",
    "@types/blessed": "^0.1.11",
    "@types/colors": "^1.2.1",
    "@types/glob": "^7.1.1",
    "@types/node": "^10.12.0",
    "@types/prettyjson": "0.0.28",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@types/react-highlight": "^0.12.1",
    "atom-languageclient": "github:g0t00/atom-languageclient",
    "atom-package-deps": "^4.6.2",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.16",
    "colors": "^1.3.3",
    "escape-string-regexp": "^1.0.5",
    "glob": "^7.1.4",
    "husky": "^1.1.2",
    "json-diff": "^0.5.4",
    "lite-event": "^1.0.5",
    "nsfw": "^1.2.5",
    "prettyjson": "^1.2.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-highlight": "^0.12.0",
    "threads": "^0.12.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typed-promisify": "^0.4.0",
    "typescript": "^3.5.1",
    "vscode-languageserver": "^5.2.1",
    "vscode-languageserver-types": "^3.14.0"
  },
  "package-deps": [
    "linter",
    "hyperclick"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    },
    "hyperclick": {
      "versions": {
        "0.1.0": "provideHyper"
      }
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    }
  },
  "devDependencies": {}
}
